{
  "manifest_version": 2,

  "name": "AdultContentHider",
  "description": "Trying to save your Children and help them to use internet wisely.",
  "version": "1.0",
  "icons": { 
          "96": "Extension-logo.png" },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery-3.4.0.min.js", "hide-content.js"],
      "run_at": "document_end"
    }
  ]
}
